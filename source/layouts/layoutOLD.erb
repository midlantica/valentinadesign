
<!doctype html>
<html>
  <head>
    <meta charset="utf-8">
    
    <!-- Always force latest IE rendering engine or request Chrome Frame -->
    <meta content="IE=edge,chrome=1" http-equiv="X-UA-Compatible">
    
    <!-- Use title if it's in the page YAML frontmatter -->
    <title><%= data.page.title || "Valentina Art - Inspired and Inspirational" %></title>

        
    <%= stylesheet_link_tag "fonts.css" %>
    <%= stylesheet_link_tag "styles.css" %>
    <%= stylesheet_link_tag "lightbox.css" %>

    <%= javascript_include_tag "all" %>
    <%# javascript_include_tag "http://ajax.googleapis.com/ajax/libs/jquery/1/jquery.js" %>
    <%= javascript_include_tag "vendor/jquery-1.8.0.min.js" %>
    <%= javascript_include_tag "vendor/lightbox.js" %>


  </head>
  <body id="<%= page_classes %>">

    <div id ="wrapper">
      <div id ="innerwrapper">
        <%= partial "header" %>
        <%= partial "navigation" %>
            <%= yield %>
        <%= partial "footer" %>
      </div><!-- <<< innerwrapper -->
    </div><!-- <<< wrapper -->
  </body>
</html>